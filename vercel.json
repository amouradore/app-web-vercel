{
  "buildCommand": "cd webapp && npm install && npm run build",
  "outputDirectory": "webapp/build",
  "installCommand": "cd webapp && npm install",
  "env": {
    "REACT_APP_API_BASE": "https://app-web-vercel-production.up.railway.app"
  },
  "headers": [
    {
      "source": "/(.*)",
      "headers": [
        {
          "key": "Access-Control-Allow-Origin",
          "value": "*"
        }
      ]
    }
  ]
}
